include <stdio.h>
     
   main()
    {
       int i,t=1, sum = 0, remainder,result;
     
       
     
       
       for(i=1; i<100; i++)
        {
           t = i*t;
           result =t;
        }
     
       while (result != 0)
        {
          remainder = result % 10;
          sum       = sum + remainder;
          result         = result/ 10;
        }
     
       printf("Sum of digits  in 100! is   = %d\n", result);
     
       
    }
